{
  "name": "savadoro",
  "productName": "Savadoro",
  "version": "0.2.2",
  "description": "Pomodoro Timer by SavvySoftWorks",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=DEV electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:SavvySoftWorksLLC/savadoro_electron.git"
  },
  "keywords": [],
  "author": "garyanewsome, arevyn24",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg"
        ],
        "linux": [
          "deb"
        ]
      },
      "electronPackagerConfig": {
        "name": "Savadoro",
        "icon": "res/icon.icns",
        "asar": true,
        "osxSign": true,
        "quiet": true
      },
      "electronWinstallerConfig": {
        "name": "Savadoro"
      },
      "electronInstallerDebian": {},
      "github_repository": {
        "owner": "SavvySoftWorksLLC",
        "name": "Savadoro"
      },
      "windowsStoreConfig": {
        "packageName": "Savadoro",
        "name": "savadoro_electron"
      }
    }
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "c3": "^0.4.15",
    "electron-compile": "^6.4.1",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "electron-prebuilt-compile": "1.6.11",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0"
  }
}
